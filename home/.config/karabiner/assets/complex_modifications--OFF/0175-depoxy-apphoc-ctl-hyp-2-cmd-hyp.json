{
  "title": "#0175 — DepoXy Ambers Launchers",
  "rules": [

    {
      "description": "#0176 — Chrome, iTerm2 — Remap <Ctrl--> → <Cmd--> (to Zoom Out)",
      "manipulators": [
        {
          "from": {
            "key_code": "hyphen",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": {
            "key_code": "hyphen",
            "modifiers": [ "left_command" ]
          },
          "conditions": [{
            "type": "frontmost_application_if",
            "bundle_identifiers": [
              "^com\\.google\\.Chrome",
              "^com\\.googlecode\\.iterm2"
            ]
          }],
          "type": "basic"
        }
      ]
    }

  ]
}
